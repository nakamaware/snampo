{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Flutter dev",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "cwd": "${workspaceFolder}/frontend",
            "toolArgs": [
                "--dart-define=FLAVOR=dev",
                "--dart-define=API_BASE_URL=http://localhost:8000"
            ]
        },
        {
            "name": "Flutter prod",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "cwd": "${workspaceFolder}/frontend",
            "toolArgs": [
                "--dart-define=FLAVOR=prod",
                "--dart-define=API_BASE_URL=${env:FLUTTER_API_BASE_URL_PROD}"
            ]
        }
    ]
}
